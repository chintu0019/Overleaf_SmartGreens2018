journal = {Proceedings of the 8th ACM international conference on Autonomic computing - ICAC '11},
keywords = {all or part of,analytics,cloud,data center,is granted without fee,monitoring,or hard copies of,permission to make digital,personal or classroom use,provided that copies are,this work for},
pages = {141},
title = {{A flexible architecture integrating monitoring and analytics for managing large-scale data centers}},
url = {http://portal.acm.org/citation.cfm?doid=1998582.1998605},
year = {2011}
}
@article{Sun2012,
abstract = {Analytical solutions are considered as increasingly important for modern enterprises. Currently, systematical adoption of analytical solutions is limited to only a small set of large enterprises, as the deployment cost is high due to high performance hardware requirement and expensive analytics software. Moreover, such on-premises solutions are not suitable for the occasional analytics consumers. In order to accelerate the prevalence of analytical solutions, this paper explores the feasibility of leveraging SaaS (Software-as-a-Service) delivery model to provide analytics capabilities as services in a cost-effective way. The main contributions of our work include: (1) proposing a framework to enable enterprise tenants to consume analytics capabilities as services; (2) developing a method to enhance existing analytics platform to support multi-tenancy so that a single software instance can effectively support multiple concurrent tenants; (3) designing an SLA (Service Level Agreement) customization mechanism to satisfy the diverse analytics capability demands of tenants. A prototype system has been developed to evaluate the feasibility of our approach.},
annote = {Important Reference},
author = {Sun, Xi and Gao, Bo and Fan, Liya and An, Wenhao},
doi = {10.1109/ICWS.2012.79},
file = {:Users/dnductien/Documents/Mendeley Desktop/Sun et al.{\_}2012.pdf:pdf},
isbn = {9780769547527},
journal = {Proceedings - 2012 IEEE 19th International Conference on Web Services, ICWS 2012},
keywords = {analytics,multi-tenancy,software as a service},
number = {1},
pages = {512--519},
title = {{A cost-effective approach to delivering analytics as a service}},
year = {2012}
}
