@article{Wang2011,
abstract = {To effectively manage large-scale data centers and utility clouds,$\backslash$noperators must understand current system and application behaviors.$\backslash$nThis requires continuous, real-time monitoring along with on-line$\backslash$nanalysis of the data captured by the monitoring system, i.e., integrated$\backslash$nmonitoring and analytics -- Monalytics [28]. A key challenge with$\backslash$nsuch integration is to balance the costs incurred and associated$\backslash$ndelays, against the benefits attained from identifying and reacting$\backslash$nto, in a timely fashion, undesirable or non-performing system states.$\backslash$nThis paper presents a novel, flexible architecture for Monalytics$\backslash$nin which such trade-offs are easily made by dynamically constructing$\backslash$nsoftware overlays called Distributed Computation Graphs (DCGs) to$\backslash$nimplement desired analytics functions. The prototype of Monalytics$\backslash$nimplementing this flexible architecture is evaluated with motivating$\backslash$nuse cases in small scale data center experiments, and a series of$\backslash$nanalytical models is used to understand the above trade-offs at large$\backslash$nscales. Results show that the approach provides the flexibility needed$\backslash$nto meet the demands of autonomic management at large scale with considerably$\backslash$nbetter performance/cost than traditional and brute force solutions.},
author = {Wang, Chengwei and Schwan, Karsten and Talwar, Vanish and Eisenhauer, Greg and Hu, Liting and Wolf, Matthew},
doi = {10.1145/1998582.1998605},
file = {:Users/dnductien/Documents/Mendeley Desktop/Wang et al.{\_}2011.pdf:pdf},
